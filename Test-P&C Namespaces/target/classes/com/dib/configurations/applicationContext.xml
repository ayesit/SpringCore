<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="course" class="com.dib.beans.Course" c:cname="Java" c:cno="1001" c:fee="4000"></bean>

<bean id="date" class="java.util.Date" p:year="98" p:month="7" p:date="6"></bean>

<bean id="student" class="com.dib.beans.Student" p:sname="Dibya" p:sno="101" p:course-ref="course" p:date-ref="date"></bean>

</beans>
